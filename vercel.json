{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "api/**/*.ts",
      "use": "@vercel/node"
    },
    {
      "src": "prisma/**/*",
      "use": "@prisma/vercel"
    }
  ],
  "version": 2
}
